	case I is
		when "000" =>
		O <= "00000001";
		when "001" =>
		O <= "00000010";
		when "010" =>
		O <= "00000100";
		when "011" =>
		O <= "00001000";
		when "100" =>
		O <= "00010000";
		when "101" =>
		O <= "00100000";
		when "110" =>
		O <= "01000000";
		when "111" =>
		O <= "10000000";
	end case;
	end process;
